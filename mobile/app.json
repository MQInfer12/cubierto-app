{
  "expo": {
    "name": "Cubierto",
    "slug": "cubierto",
    "scheme": "cubierto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ff361a"
